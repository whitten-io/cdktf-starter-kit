{
  "editor.formatOnSave": true,
  "workbench.colorTheme": "Solarized Light Alternate",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.gen": true,
    "**/cdktf.out": true,
    "**/dist": true,
  },
  "eslint.enable": true,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.format.enable": true,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  }
}